<script type="text/javascript">
var f=document.getElementById('bform');
var l=f.bookingPeriod.length;
document.getElementById('submitBooking').onclick=function() {
for(var i=0;i<l;i++) {
if(f.bookingPeriod[i].checked==true) {return true;}
}
alert('Please enter the amount of time required');
return false;
}
</script>
